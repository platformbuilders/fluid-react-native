{
  "spacing": {
    "xs": 4,
    "sm": 8,
    "md": 16,
    "lg": 32,
    "xl": 96
  },
  "sizing": {
    "xs": 4,
    "sm": 8,
    "md": 16,
    "lg": 32,
    "xl": 96
  },
  "borderRadius": {
    "xs": "$borderRadius.base / 2",
    "base": 2,
    "sm": "$borderRadius.base * 2",
    "md": "$borderRadius.base * 3",
    "lg": "$borderRadius.base * 4",
    "xl": "$borderRadius.base * 6",
    "pill": 999
  },
  "borderWidth": {
    "none": 0,
    "xs": 1,
    "sm": 2,
    "md": 4,
    "lg": 6,
    "xl": 8
  },
  "boxShadow": {
    "lv1": {
      "value": {
        "x": 0,
        "y": 1,
        "spread": 0,
        "color": "rgba(0, 0, 0, 0.1)",
        "blur": 3
      }
    },
    "lv2": {
      "value": {
        "x": 0,
        "y": 4,
        "spread": -1,
        "color": "rgba(0, 0, 0, 0.1)",
        "blur": 6
      }
    },
    "lv3": {
      "value": {
        "x": 0,
        "y": 10,
        "spread": -3,
        "color": "rgba(0, 0, 0, 0.1)",
        "blur": 15
      }
    },
    "lv4": {
      "value": {
        "x": 0,
        "y": 20,
        "spread": -5,
        "color": "rgba(0, 0, 0, 0.1)",
        "blur": 25
      }
    },
    "lv5": {
      "value": {
        "x": 0,
        "y": 25,
        "spread": -12,
        "color": "rgba(0, 0, 0, 0.2)",
        "blur": 50
      }
    }
  },
