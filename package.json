    "@types/react": "^18.0.0"
  },
  "scripts": {
    "build": "rm -rf dist && yarn lint && tsc --project ./tsconfig.publish.json && yarn copyassets",
    "start": "react-native start --reset-cache",
    "start:android": "react-native run-android --no-jetifier",
    "start:ios": "react-native run-ios --project-path DevelopmentApp/ios",
    "clean": "rm -rf ./node_modules native yarn.lock dist coverage && yarn install",
    "copyassets": "npx copyfiles -f ./src/assets/images/*.png ./dist/assets/images",
    "lint": "tsc --noEmit && eslint \"src/**/*.{ts,tsx}\" --fix",
    "precommit": "yarn lint && yarn test",
    "pretty": "patch-package && prettier --config ./.prettierrc.js --write \"src/**/*.{ts,tsx,js}\"",
    "prepublish": "yarn build",
    "storybook": "start-storybook -p 9009 -c storybook",
    "storybook:build": "rimraf storybook-static; build-storybook -c storybook",
    "test": "jest --silent=false --passWithNoTests --collect-coverage",
    "test:coverage": "yarn test -- --coverage",
    "test:watch": "yarn test -- --watch",
    "preversion": "yarn test && yarn lint",
    "postversion": "git push && git push --tags",
    "version:major": "npm version major",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn pretty",
      "yarn lint",
      "yarn test"
    ]
  },
