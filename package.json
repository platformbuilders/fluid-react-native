{
  "name": "@platformbuilders/fluid-react-native",
  "version": "1.0.0",
  "private": false,
  "description": "Builders React Native for Fluid Design System",
  "keywords": [
    "platformbuilders",
    "react native",
    "fluid",
    "components"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/platformbuilders/fluid-react-native.git"
  },
  "author": "Platform Builders <tecnologia@platformbuilders.io>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "resolutions": {
    "@types/react": "18.0.0"
  },
  "scripts": {
    "build": "patch-package && rm -rf dist && yarn lint && tsc --project ./tsconfig.publish.json && yarn copyassets",
    "copyassets": "npx copyfiles -f ./src/assets/images/*.png ./dist/assets/images",
    "android": "react-native run-android",
    "clean": "rm -rf ./node_modules native yarn.lock dist coverage && yarn install",
    "ios": "react-native run-ios",
    "lint": "tsc --noEmit && eslint \"src/**/*.{ts,tsx}\" --fix",
    "precommit": "yarn lint && yarn test",
    "pretty": "patch-package && prettier --config ./.prettierrc.js --write \"src/**/*.{ts,tsx,js}\"",
    "prepublish": "yarn build",
    "start": "react-native start",
    "storybook:generate": "sb-rn-get-stories -js",
    "test": "patch-package && jest --silent=false --passWithNoTests --collect-coverage",
    "test:coverage": "yarn test -- --coverage",
    "test:watch": "yarn test -- --watch",
    "preversion": "yarn test && yarn lint",
    "postversion": "git push && git push --tags",
    "version:major": "npm version major",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn pretty",
      "yarn lint",
      "yarn test"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.26.0",
    "@testing-library/jest-native": "5.4.3",
    "@testing-library/react-native": "12.8.1",
    "numeral": "2.0.6",
