  "scripts": {
    "clean": "rm -rf ./node_modules native yarn.lock dist coverage && yarn install",
    "pretty": "prettier --config ./.prettierrc.js --write \"src/**/*.{ts,tsx,js}\"",
    "lint": "tsc --noEmit && eslint \"src/**/*.{ts,tsx}\" --fix",
    "test": "jest --passWithNoTests --collect-coverage",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "yarn test -- --coverage",
    "build": "rm -rf dist && yarn lint && tsc && yarn copyassets",
    "copyassets": "npx copyfiles -f ./src/assets/images/*.png ./dist/assets/images",
    "prepublish": "yarn test && yarn build",
    "precommit": "yarn test && yarn lint",
    "preversion": "yarn test && yarn lint",
    "postversion": "git push && git push --tags",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "storybook": "start-storybook -p 9009 -c storybook",
    "storybook:build": "rimraf storybook-static; build-storybook -c storybook"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn pretty",
      "yarn lint",
      "yarn test"
    ]
  },
